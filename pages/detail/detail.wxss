.navWrapper{
  position: fixed;  
  height: 160rpx;
  width: 100%;
  z-index: 66;
  border-bottom:3rpx solid #2b4837;
  background-color: white;
}

.backIcon{
  position: absolute;
  top: 83rpx;
  left: 38rpx;
  width: 30rpx;
  height: 30rpx; 
}

.navTitle{
  position: absolute;
  top:70rpx;
  left:80rpx;
  font-family: "HYXinRenWenSongW",汉仪新人文宋,"Microsoft YaHei",微软雅黑;
  font-size: 40rpx;
  line-height: 56rpx;
  color: #2b4837;
}

.allContent {
  position: absolute;
  top: 160rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding-bottom:100rpx;
}

.head {
  margin-top: 30rpx;
}

.title {
  font-size: 40rpx;
  line-height: 56rpx;
